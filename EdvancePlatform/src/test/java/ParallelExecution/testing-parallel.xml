<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >
<suite name="ParallelSuite" parallel="tests" thread-count="2">

    <!--    Parallel Execution for UniversityStudentProfileTest from both Chrome and Edge-->
    <test name="UniversityStudentProfileTestChrome">
        <parameter name="browser" value="chrome"/>
        <classes>
            <class name="PagesTests.UniversityStudentProfileTest"/>
        </classes>
    </test>
    <test name="UniversityStudentProfileTestEdge">
        <parameter name="browser" value="edge"/>
        <classes>
            <class name="PagesTests.UniversityStudentProfileTest"/>
        </classes>
    </test>


